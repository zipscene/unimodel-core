{
  "name": "zs-unimodel",
  "version": "0.0.1",
  "description": "Basis for zsapi unified model framework",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@git.zipscene.com:zsapilibs/zs-unimodel.git"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "5.6.19",
    "crisphooks": "^2.0.0",
    "inheritz": "^1.1.0",
    "lodash": "^3.10.0",
    "pasync": "^1.1.0",
    "xerror": "^1.0.0",
    "zs-common-query": "^1.4.1",
    "zs-common-schema": "^1.4.0",
    "zs-objtools": "^1.1.0",
    "zstreams": "^3.2.1"
  },
  "devDependencies": {
    "broccoli": "^0.16.4",
    "broccoli-babel-transpiler": "^5.2.3",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.0.0",
    "es6-promise": "^2.3.0",
    "eslint": "^0.24.1",
    "mocha": "^2.2.5",
    "source-map-support": "^0.3.2",
    "yuidocjs": "^0.7.0"
  },
  "publishConfig": {
    "registry": "https://npm.zipscene.com/"
  }
}
