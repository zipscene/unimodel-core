{
  "name": "unimodel-core",
  "version": "0.6.14",
  "description": "Universal unified database model framework, core package",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/unimodel-core.git"
  },
  "keywords": [
    "unimodel"
  ],
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "^5.8.29",
    "crisphooks": "^2.0.0",
    "inheritz": "^1.1.0",
    "lodash": "^3.10.0",
    "pasync": "^1.4.0",
    "xerror": "^1.0.0",
    "zstreams": "^3.2.2",
    "common-query": "^1.9.11",
    "common-schema": "^1.7.13",
    "objtools": "^1.6.2"
  },
  "devDependencies": {
    "broccoli": "^0.16.7",
    "broccoli-babel-transpiler": "^5.4.3",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^0.2.3",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.2.1",
    "mocha": "^2.2.5",
    "source-map-support": "^0.3.2",
    "yuidocjs": "^0.9.0"
  },
  "license": "Apache-2.0"
}